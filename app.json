  "expo": {
    "name": "ShopAI",
    "slug": "ShopAI",
    "version": "1.0.5",
    "scheme": "ShopAI",
    "platforms": [
      "ios",
      "android"
    ],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      [
        "expo-camera",
        {
          "cameraPermission": "Shop AI needs access to your camera to scan products and find the best deals."
        }
      ],
