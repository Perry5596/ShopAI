        "expo-image-picker",
        {
          "photosPermission": "Shop AI needs access to your photos to select or upload product images."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#000000"
        }
      ],
